
public class DoMath1 {

	public static void main(String[] args) {
		
		System.out.println(Calculate.square(5));
		System.out.println(Calculate.cube(5));
		System.out.println(Calculate.average(5,3));
		System.out.println(Calculate.average(2.0,3.0,4.0));
		System.out.println(Calculate.toDegrees(3.14159));
		System.out.println(Calculate.toRadians(180));
		System.out.println(Calculate.discriminant(1,2,3));
		System.out.println(Calculate.toImproperFrac(3,1,2));
		System.out.println(Calculate.toMixedNum(7,2));
		System.out.println(Calculate.foil(1,2,3,4));
		System.out.println(Calculate.isDivisibleBy(1, 2));
		System.out.println(Calculate.absValue(-3.2));
		System.out.println(Calculate.max(3,4));
		System.out.println(Calculate.max(3,4,5));
		System.out.println(Calculate.min(3,4));
		System.out.println(Calculate.round2(6.449));
		System.out.println(Calculate.exponent(2.5,2));
		System.out.println(Calculate.factorial(5));
		System.out.println(Calculate.isPrime(25));
		System.out.println(Calculate.gcf(20,36));
		System.out.println(Calculate.sqrt(8.9));
		System.out.println(Calculate.quadForm(2,2,2));
		
		
		
		
	}
}
